<div class="row">
				

		
<div class="col-md-8">

			<form action="search" style="padding-top:12px;">
				<div class="input-group">
					<%= text_field_tag("search",@search_key,
								                   :class => 'autocomplete-input form-control',
								                   :autocomplete => 'on',
								                   :placeholder  => 'Search ..',
								                   :'data-url'   => "search/auto_complete_search"
								                   )
					%>
			      <span class="input-group-btn">
			        <button class="btn btn-default" type="button">Search!</button>
			      </span>
			    </div>
			</form>





		
	<div class="col-md-12 feat animatedParent" data-sequence="100">
		<div class="thumbnail hvr-box-shadow-outset">
			<div class="animated bounceInLeft" data-id='1'>
				<div class="panel-heading animated bounceInLeft" data-id='1'><i class="fa fa-star" style="font-size: 20px;"></i> <span class="pull-right">This Week's featured builds</span></div>
				
				  <%bounceseq=0%>
				<% @items.each do |item|%>
				    <div class="hvr-box-shadow-outset" style="z-index: <%= 991-bounceseq %>; position: relative;width:100%;">
						<div class="col-md-12 thumbnail animated bounceInLeft" data-id='<%=bounceseq+2%>'>
							<div class="col-md-3 feat-thumb">
								<img data-src="holder.js/100%x200" alt="100%x200" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 100%; display: block;">
							</div>
					        <div class="col-md-9 caption">
					        	<h4><%= link_to item.product,eval("#{item.class.name.underscore.downcase}_path(#{item.id})") %></h4>
						        <p><%=item.as_json.except("id","").select{|k,v| !v.nil? && !v.blank? && !k.end_with?("_id") && !k.end_with?("_at") }.map{|k,v| "#{k} : #{v}"}.join(", ")%> </p>
						     </div>
				     	</div>
				  	</div>
				  <% bounceseq=bounceseq+1%>
				  <% end %>
				  	<div class="panel-footer text-right"><span><%= will_paginate @items %></span></div>
				  <style>
				  .pagination {
				  	float:none;
				  }
				  .panel-footer{padding:0;}
				  </style>



				  
  
			   </div>
			   
			</div>
			
		</div>










			</div>
		</div>
