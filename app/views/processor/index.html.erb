<p id="notice"><%= notice %></p>

<h1>Listing Processor Processors</h1>

<table>
  <thead>
    <tr>
      <th>Processor number</th>
      <th>Cache in mb</th>
      <th>Dmi2 in gts</th>
      <th>Instruction set in bit</th>
      <th>Instruction set extensions</th>
      <th>Embedded options available</th>
      <th>Lithography in nm</th>
      <th>Scalability</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>


    <% @processor_processors.each do |processor_processor| %>
      <tr>
        <td><%= processor_processor.processor_number %></td>
        <td><%= processor_processor.cache_in_MB %></td>
        <td><%= processor_processor.DMI2_in_GTs %></td>
        <td><%= processor_processor.instruction_set_in_bit %></td>
        <td><%= processor_processor.instruction_set_extensions %></td>
        <td><%= processor_processor.embedded_options_available %></td>
        <td><%= processor_processor.lithography_in_nm %></td>
        <td><%= processor_processor.scalability %></td>
        <td><%= link_to 'Show', processor_processor %></td>
        <td><%= link_to 'Edit', edit_processor_processor_path(processor_processor) %></td>
        <td><%= link_to 'Destroy', processor_processor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>



  </tbody>
</table>

<br>

<%= link_to 'New Processor', new_processor_processor_path %>
