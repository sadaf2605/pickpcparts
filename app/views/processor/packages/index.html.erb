<p id="notice"><%= notice %></p>

<h1>Listing Processor Packages</h1>

<table>
  <thead>
    <tr>
      <th>Max cpu configuration</th>
      <th>Tcase in c</th>
      <th>Package width in mn</th>
      <th>Package length in mn</th>
      <th>Graphics and imc lithography in nm</th>
      <th>Low halogen options available</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @processor_packages.each do |processor_package| %>
      <tr>
        <td><%= processor_package.max_cpu_configuration %></td>
        <td><%= processor_package.TCASE_in_C %></td>
        <td><%= processor_package.package_width_in_mn %></td>
        <td><%= processor_package.package_length_in_mn %></td>
        <td><%= processor_package.graphics_and_imc_lithography_in_nm %></td>
        <td><%= processor_package.low_halogen_options_available %></td>
        <td><%= link_to 'Show', processor_package %></td>
        <td><%= link_to 'Edit', edit_processor_package_path(processor_package) %></td>
        <td><%= link_to 'Destroy', processor_package, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Package', new_processor_package_path %>
